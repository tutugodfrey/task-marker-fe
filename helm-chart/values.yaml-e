namespace:
  name: dev

deployment:
  name: todoapp-fe-deployment
  replicaCount: 1
   
container:
  name: todoapp-fe-container
  image: tutug/task-marker-fe
  tag: 77
  containerPort: 80

selector:
  app: todoapp-fe

service:
  name: todoapp-fe-service
  port: 80
  targetPort: 80
  type: LoadBalancer

resources:
  requests:
    cpu: 100m
    memory: 100Mi
  limits:
    cpu: 200m
    memory: 200Mi

volumes:
  shared_logs:
    name: shared-logs
    mountPath: /var/log/nginx

